disabled_rules:
  - trailing_whitespace
  - force_cast
  - force_try
  - multiple_closures_with_trailing_closure

opt_in_rules:
  - empty_count
  - missing_docs
  - array_init

line_length: 
  warning: 120
  error: 150
  ignores_urls: true
  ignores_function_declarations: true
  ignores_comments: true

file_length:
  warning: 500
  error: 800

type_name:
  min_length: 3 
  max_length:
    warning: 40
    error: 50

identifier_name:
  min_length: 2
  max_length: 
    warning: 30
    error: 40
  excluded: ["id", "y, "x"]

cyclomatic_complexity:
  warning: 15
  error: 20

excluded:
  - Carthage
  - Pods
  - Generated

included:
  - Source
  - AnotherSour
